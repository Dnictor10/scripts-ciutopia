using UnityEngine;
using UnityEngine.SceneManagement;

public class CambiarPantalla : MonoBehaviour
{
    public string nombreDeLaEscenaAIntercambiar = "NombreDeTuOtraEscena";

    public void onClick()
    {
        CambiarEscena();
    }

    public void CambiarEscena()
    {
        SceneManager.LoadScene(nombreDeLaEscenaAIntercambiar);
    }
}
